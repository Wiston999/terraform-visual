{
  "name": "terraform-visual",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run start:backend",
    "build": "npm run build:backend && npm run build:client",
    "start:backend": "cd backend && node -r ./tsconfig-paths-bootstrap.js dist/cmd/server/index",
    "build:backend": "cd backend && rm -rf dist && npx tsc",
    "dev:backend": "cd backend && NODE_ENV=development ENV=local ts-node -P ./tsconfig.json -r tsconfig-paths/register src/cmd/server/index",
    "build:client": "cd client && rm -rf .next && next build",
    "dev:client": "cd client && next"
  },
  "author": "hieven <hieven.c@gmail.com>",
  "dependencies": {
    "@antv/g6": "^3.5.7",
    "@koa/router": "^9.0.1",
    "@octokit/rest": "^18.0.3",
    "@reduxjs/toolkit": "^1.4.0",
    "aws-sdk": "^2.703.0",
    "dotenv": "^8.2.0",
    "joi": "^17.1.1",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-compress": "^4.0.1",
    "lodash": "^4.17.15",
    "next": "^9.4.4",
    "next-redux-wrapper": "^6.0.2",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.0",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@types/joi": "^14.3.4",
    "@types/koa": "^2.11.3",
    "@types/koa-compress": "^4.0.0",
    "@types/koa__router": "^8.0.2",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "license": "MIT"
}
